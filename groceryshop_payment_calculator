import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      double armutFiyat = 2.14, elmaFiyat = 3.67, domatesFiyat = 1.11, muzFiyat = 0.95, patlicanFiyat = 5.00, toplam, armut, elma, domates, muz, patlican;
      int armutKG, elmaKG, domatesKG, muzKG, patlicanKG;

      Scanner input = new Scanner(System.in);
      System.out.print("Armut kaç kg ? : ");
      armutKG = input.nextInt();
      armut = armutKG * armutFiyat;

      System.out.print("Elma kaç kg ? : ");
      elmaKG = input.nextInt();
      elma = elmaKG * elmaFiyat;

      System.out.print("Domates kaç kg ? : ");
        domatesKG = input.nextInt();
        domates = domatesKG * domatesFiyat;

      System.out.print("Muz kaç kg ? : ");
        muzKG = input.nextInt();
        muz = muzKG * muzFiyat;

      System.out.print("Patlican kaç kg ? : ");
        patlicanKG = input.nextInt();
        patlican = patlicanKG * patlicanFiyat;

      toplam = armut + elma + domates + muz + patlican;
      System.out.print("Ödemeniz gereken fiyat : " + toplam);


    }
}
