import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      double boy, vkIndex;
      int kilo;

      Scanner input = new Scanner(System.in);
      System.out.print("Lütfen Boyunuzu (metre cinsinden, örnek: 1,75) Giriniz : ");
      boy = input.nextDouble();

      System.out.print("Lütfen Kilonuzu Giriniz : ");
      kilo = input.nextInt();

      vkIndex = kilo / (boy * boy);
      System.out.print("Vücut kitle indeksiniz : " + vkIndex);
    }
}
